{
  "name": "@liftera/ui",
  "version": "0.0.0",
  "private": true,
  "main": "./src/components/index.ts",
  "types": "./src/components/index.ts",
  "exports": {
    "./gluestack/*": {
      "types": "./src/gluestack/*/index.tsx",
      "default": "./src/gluestack/*/index.tsx"
    },
    "./components/*": {
      "types": "./src/components/*.tsx",
      "default": "./src/components/*.tsx"
    },
    "./tailwind-preset": "./tailwind-preset.js",
    "./tokens/colors": "./tokens/colors.ts",
    "./theme.css": "./theme.css"
  },
  "scripts": {
    "lint": "eslint",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/react": "~19.1.13",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "expo": "~54.0.25",
    "nativewind": "^4.1.23",
    "typescript": "~5.9.2"
  },
  "peerDependencies": {
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "dependencies": {
    "@gluestack-ui/core": "^3.0.12",
    "@gluestack-ui/utils": "^3.0.13"
  }
}
